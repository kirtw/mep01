if [ ! -f "/home/kw/km5b/yottadb/gtmy-r134"/ydb_env_set ] ; then echo Cannot find file "/home/kw/km5b/yottadb/gtmy-r134"/ydb_env_set to source
else
    . "/home/kw/km5b/yottadb/gtmy-r134"/ydb_env_set
    ydb_tmp_status=$?
    if [ 0 = $ydb_tmp_status ] ; then
	unset ydb_tmp_status
	if [ 0 = $# ] ; then
	    $ydb_dist/yottadb -direct
	elif [ "-help" = "$1" -o "-h" = "$1" -o "-?" = "$1" ] ; then
	    echo "ydb -dir[ect] to enter direct mode (halt returns to shell)"
	    echo "ydb -help / ydb -h / ydb -? to display this text"
	    echo "ydb -run <entryref> to start executing at an entryref"
	    echo "ydb -version to display version information"
	else
	    $ydb_dist/yottadb $*
	fi
    else
	echo Sourcing "/home/kw/km5b/yottadb/gtmy-r134"/ydb_env_set returned status $ydb_tmp_status
    fi
fi
